{
  "name": "sofar",
  "description": "感謝するぜ お前と出会えた これまでの 全てに!!!",
  "version": "0.0.0",
  "author": "ideyuta <ide.yuta@gmail.com>",
  "dependencies": {
    "fs-extra": "^0.26.4",
    "github-markdown-css": "^2.2.0",
    "moment": "^2.11.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.5",
    "react-document-title": "^2.0.1",
    "react-router": "^0.13.5"
  },
  "keywords": [
    "log"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideyuta/sofar.git"
  },
  "scripts": {
    "build": "mkdir -p public/ && npm-run-all build:css build:gatsby",
    "build:css": "stylus -c styles/base.styl --include-css -I node_modules -o public/bundle.css",
    "build:gatsby": "gatsby build --prefix-links",
    "deploy": "gh-pages -d public",
    "develop": "gatsby develop",
    "lint": "eslint --cache pages/*.jsx",
    "start": "npm-run-all --parallel develop watch:*",
    "test": "mocha test/index.js --compilers js:babel/register",
    "watch:css": "chokidar styles/ -c 'npm run build:css'",
    "watch:lint": "chokidar pages/ -c 'npm run -s lint'"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "chokidar-cli": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-kanmu": "^9.1.0",
    "eslint-plugin-react": "^3.13.1",
    "eslint-plugin-sorting": "0.0.1",
    "gh-pages": "^0.8.0",
    "npm-run-all": "^1.4.0",
    "stylus": "^0.53.0"
  }
}
